<!DOCTYPE html>
<html>
<head>
	<title>NubeTewb</title>
	<%= stylesheet_link_tag 'application', media: 'all' %>
	<%= csrf_meta_tags %>
</head>
<body>

	<%= form_tag("/search", method: "get", class: "search-bar") do %>
	<%= label_tag(:q, "Search for:") %>
	<%= text_field_tag(:q) %>
	<%= submit_tag("Search") %>
	<% end %>


	<div id="timer">
	</div>
	<div id="category-container">
	</div>
	

	<%= yield %>

	<%= javascript_include_tag 'application' %>
</body>
</html>
